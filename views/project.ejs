    <form method="POST" action="/save/<%= _id %>">
      <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog">
        
          <!-- Modal content-->
          <div class="modal-content">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal">&times;</button>
              <h4 class="modal-title">The Project will be Completed</h4>
            </div>
            <div class="modal-body">
              <p>Remarks</p>
              <textarea name="remarks" id="remarks" cols="50" rows="3"></textarea>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-default" data-dismiss="modal" id="projClose">Close</button>
              <button type="submit" class="btn btn-default"><a class="projSave" id="projSaved">Save</a></button>
            </div>
          </div>
          
        </div>
      </div>
    </form>

